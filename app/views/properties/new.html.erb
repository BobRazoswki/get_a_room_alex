<% form_for @property do |f| %>

	<%= f.label :title %>
	<%= f.text_field :title %>

	<%= f.label :address %>
	<%= f.text_field :address %>

	<%= f.label :city %>
	<%= f.text_field :city %>

	<%= f.label :postcode %>
	<%= f.text_field :postcode %>

	<%= f.label :description %>
	<%= f.text_field :description %>

	<%= f.submit "Submit" %> 

<% end %>